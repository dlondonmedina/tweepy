<p id="notice"><%= notice %></p>

<h1>Tweeps</h1>

<table>
  <thead>
    <tr>
      <th>Tweep</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tweeps.each do |tweep| %>
      <tr>
        <td><%= tweep.tweep %></td>
        <td><%= link_to 'Show', tweep %></td>
        <td><%= link_to 'Edit', edit_tweep_path(tweep) %></td>
        <td><%= link_to 'Destroy', tweep, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tweep', new_tweep_path %>
